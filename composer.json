{
    "name": "taskb/user-discounts",
    "description": "A reusable Laravel package for user-level discounts with deterministic stacking and full audit trail",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["laravel", "package", "discounts", "user-discounts", "ecommerce"],
    "authors": [
        {
            "name": "TaskB Developer",
            "email": "developer@taskb.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "TaskB\\UserDiscounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TaskB\\UserDiscounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaskB\\UserDiscounts\\Providers\\DiscountServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
